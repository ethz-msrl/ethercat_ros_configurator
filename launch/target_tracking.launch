
<launch>


    <!--    This launch files first set-ups the entire system by configurating this system according the setup.yaml which homes the system
            Afterwards, the system begins the inverse kinematic node and motor_msg_conversion node-->


    <node name="calibration" pkg="ethercat_ros_configurator" type="test_reference_generator.py" output="screen">
        <rosparam file="$(find ethercat_ros_configurator)/config/setup.yaml" command="load"/>
    </node>

    <node name = "inverse_kinematic" pkg = "ethercat_ros_configurator" type = "inverse_kinematic.py" output = "screen">
    </node>

    <node name = "motor_msg_conversion" pkg = "ethercat_ros_configurator" type = "motor_msg_conversion.py" output = "screen">
    </node>



</launch>
